<div class="profile">

      <img src="<%= @content.image.url(:thumb) %>">
      <div class="form-group">
        <label>Name:  </label><br/><%= @user.profile[:name] %>
      </div>
      <div class="form-group">
      <label>Last Name:  </label><br/><%= @user.profile[:last_name] %>
      </div>
      <div class="form-group">
      <label>About Me:  </label><br/><%= @user.profile[:about_me] %>
      </div>
    <table class="table">
      <tr><th>Project Title</th></tr>
      <% @user.projects.each do |project| %>
        <tr><td><%= link_to project[:title] ,project_url(project) %></td></tr>
      <% end %>
    </table>

</div>